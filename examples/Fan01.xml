<?xml version='1.0' encoding='UTF-8' ?>

<functy>
	<settings>
		<show-axes bool="0"/>
		<invert bool="0"/>
		<wireframe bool="0"/>
	</settings>
	<range>
		<min>
			<coord x="-0.079344" y="-0.079344" z="-0.079344"/>
		</min>
		<width>
			<coord x="0.158687" y="0.158687" z="0.158687"/>
		</width>
	</range>
	<function type="spherical">
		<equation>(0.01*((1+cos(12*(a+(p*2)))*sin(p))+(1+cos(3*a+p))))</equation>
